{
  "name": "yokai",
  "version": "0.0.0",
  "author": {
    "email": "geoffrey.testelin@gmail.com",
    "name": "Geoffrey 'C0ZEN' Testelin",
    "url": "https://github.com/C0ZEN"
  },
  "contributors": [
    {
      "email": "geoffrey.testelin@gmail.com",
      "name": "Geoffrey 'C0ZEN' Testelin",
      "url": "https://github.com/C0ZEN"
    }
  ],
  "description": "The Sonia Yokai game",
  "homepage": "https://github.com/Sonia-corporation/yokai",
  "maintainers": [
    {
      "email": "geoffrey.testelin@gmail.com",
      "name": "Geoffrey 'C0ZEN' Testelin",
      "url": "https://github.com/C0ZEN"
    }
  ],
  "license": "MIT",
  "keywords": [
    "sonia",
    "yokai",
    "angular"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Sonia-corporation/yokai.git"
  },
  "bugs": {
    "url": "https://github.com/Sonia-corporation/yokai/issues"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "./scripts/commitizen.js"
    }
  },
  "engines": {
    "node": ">=12.x",
    "npm": ">=6"
  },
  "private": false,
  "files": [
    "dist/*",
    "CHANGELOG.md",
    "CODE_OF_CONDUCT.md",
    "CONTRIBUTING.md",
    "LICENSE",
    "package.json",
    "package-lock.json",
    "README.md"
  ],
  "main": "./apps/game/src/main.ts",
  "scripts": {
    "\n# RUN SCRIPTS:": "",
    "start": "nx serve -c dev",
    "build:prod": "nx build --prod",
    "\n# TEST SCRIPTS:": "",
    "test": "nx test",
    "test:ci": "nx test --ci --clear-cache --code-coverage=false",
    "test:ci:coverage": "nx test --ci --clear-cache --code-coverage=true",
    "e2e": "nx e2e",
    "e2e:ci": "nx e2e --headless=true --parallel=true",
    "test:mutant": "stryker run",
    "test:mutant:ci": "stryker run --fileLogLevel fatal --logLevel fatal --reporters clear-text,progress,dashboard",
    "\n# LINT SCRIPTS:": "",
    "lint": "nx workspace-lint && nx lint --cache --cache-location .eslintcache",
    "lint:fix": "nx workspace-lint && nx lint --fix --cache --cache-location .eslintcache",
    "lint:ci": "nx workspace-lint && nx lint",
    "format": "nx format:write",
    "format:write": "nx format:write --verbose",
    "format:write:ci": "nx format:write --verbose --uncommitted",
    "format:check": "nx format:check --verbose",
    "format:check:ci": "nx format:check --verbose --uncommitted",
    "\n# UTILS SCRIPTS:": "",
    "affected:apps": "nx affected:apps --base=develop",
    "affected:libs": "nx affected:libs --base=develop",
    "affected:build": "nx affected:build --base=develop",
    "affected:e2e": "nx affected:e2e --base=develop",
    "affected:test": "nx affected:test --base=develop",
    "affected:lint": "nx affected:lint --base=develop",
    "affected:dep-graph": "nx affected:dep-graph --base=develop",
    "affected": "nx affected --base=develop",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "postinstall": "is-ci || husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "ng": "nx",
    "nx": "nx",
    "cz": "git add . && git-cz && npm run git:push",
    "semver": "semantic-release",
    "commit": "npm run cz",
    "git:push": "git push-upstream",
    "contributors:add": "all-contributors add",
    "contributors:generate": "all-contributors generate"
  },
  "dependencies": {
    "@angular/animations": "^11.0.0",
    "@angular/common": "^11.0.0",
    "@angular/compiler": "^11.0.0",
    "@angular/core": "^11.0.0",
    "@angular/forms": "^11.0.0",
    "@angular/platform-browser": "^11.0.0",
    "@angular/platform-browser-dynamic": "^11.0.0",
    "@angular/router": "^11.0.0",
    "@nrwl/angular": "11.2.10",
    "rxjs": "~6.6.3",
    "tslib": "^2.0.0",
    "zone.js": "^0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.1102.11",
    "@angular-eslint/eslint-plugin": "1.0.0",
    "@angular-eslint/eslint-plugin-template": "1.0.0",
    "@angular-eslint/template-parser": "1.0.0",
    "@angular/cli": "11.0.7",
    "@angular/compiler-cli": "11.1.2",
    "@angular/language-service": "11.1.2",
    "@ngneat/spectator": "7.1.0",
    "@nrwl/cli": "11.2.10",
    "@nrwl/cypress": "11.2.10",
    "@nrwl/eslint-plugin-nx": "11.2.10",
    "@nrwl/jest": "11.2.10",
    "@nrwl/linter": "11.2.10",
    "@nrwl/nx-cloud": "latest",
    "@nrwl/tao": "11.2.10",
    "@nrwl/workspace": "11.2.10",
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/git": "9.0.0",
    "@semantic-release/github": "7.2.0",
    "@sonia-corporation/conventional-changelog": "1.0.1",
    "@stryker-mutator/core": "5.0.0",
    "@stryker-mutator/jest-runner": "5.0.0",
    "@stryker-mutator/typescript-checker": "5.0.0",
    "@types/jest": "26.0.8",
    "@types/node": "12.12.38",
    "@typescript-eslint/eslint-plugin": "4.15.2",
    "@typescript-eslint/parser": "4.15.2",
    "all-contributors-cli": "6.20.0",
    "commitizen": "4.2.4",
    "cypress": "6.4.0",
    "cz-customizable": "6.3.0",
    "cz-customizable-ghooks": "2.0.0",
    "dotenv": "6.2.0",
    "eslint": "7.19.0",
    "eslint-config-google": "0.14.0",
    "eslint-config-prettier": "7.2.0",
    "eslint-config-recommended": "4.1.0",
    "eslint-import-resolver-typescript": "2.3.0",
    "eslint-plugin-cypress": "2.11.2",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.1.5",
    "eslint-plugin-jsdoc": "31.6.1",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-progress": "0.0.1",
    "eslint-plugin-rxjs": "3.1.5",
    "eslint-plugin-typescript-sort-keys": "1.5.0",
    "husky": "5.1.3",
    "install": "0.13.0",
    "is-ci": "2.0.0",
    "jest": "26.6.3",
    "jest-extended": "0.11.5",
    "jest-preset-angular": "8.3.2",
    "lint-staged": "10.5.4",
    "npm": "7.6.3",
    "pinst": "2.1.4",
    "prettier": "2.2.1",
    "semantic-release": "17.4.2",
    "stryker-cli": "1.0.0",
    "ts-jest": "26.5.3",
    "ts-node": "9.1.1",
    "typescript": "4.1.5"
  }
}
